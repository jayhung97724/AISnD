<UserControl x:Class="el.cl.createDiet_root"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:el.cl"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="1288.704">
    <Grid>
        <local:createDiet x:Name="cd" Visibility="Hidden" Height="713" Margin="0,78,10,-191" VerticalAlignment="Top" Width="Auto" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True"/>
        <local:createDiet_week x:Name="cd_week" Height="713" Margin="0,78,93,-191" VerticalAlignment="Top" Width="Auto" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True"/>
        <Button x:Name="btn_month" Content="月" HorizontalAlignment="Left" Margin="824,78,0,0" VerticalAlignment="Top" Width="75" Click="btn_month_Click"/>
        <Button x:Name="btn_week" Content="週" HorizontalAlignment="Left" Margin="904,78,0,0" VerticalAlignment="Top" Width="75" Click="btn_week_Click"/>
    </Grid>
</UserControl>
